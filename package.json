{
  "name": "visual-studio-code-angular-snippet-generator",
  "version": "1.0.0",
  "description": "A Visual Studio Code extension to generate Angular code snippets.",
  "main": "index.js",
  "publisher": "coderrob",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "scripts": {
    "test": "jest",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "watch": "tsc -watch -p ./",
    "generate:folder": "node ./lib/cli.js generate:folder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coderrob/visual-studio-code-angular-snippet-generator.git"
  },
  "keywords": [
    "vscode",
    "angular",
    "code",
    "snippets",
    "angular",
    "snippets"
  ],
  "type": "commonjs",
  "author": "Rob Lindley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coderrob/visual-studio-code-angular-snippet-generator/issues"
  },
  "homepage": "https://github.com/Coderrob/visual-studio-code-angular-snippet-generator#readme",
  "dependencies": {
    "pino": "^8.0.0",
    "pino-pretty": "^9.0.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.18.9",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.4.1",
    "commander": "^10.0.0",
    "eslint": "^8.12.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "fast-glob": "^3.2.12",
    "jest": "^29.4.1",
    "jest-runner": "^29.4.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1"
  }
}
